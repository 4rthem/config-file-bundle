{
    "name": "arthem/config-file-bundle",
    "description": "Inject files from env vars in your Symfony configuration",
    "keywords": [
        "env",
        "file",
        "docker"
    ],
    "type": "symfony-bundle",
    "require": {
        "php": "^8.2",
        "symfony/dependency-injection": "^6 || ^7",
        "symfony/http-kernel": "^6 || ^7",
        "symfony/config": "^6 || ^7"
    },
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Arthem\\ConfigFileBundle\\": "src/"
        }
    },
    "scripts": {
        "cs": "vendor/bin/php-cs-fixer fix",
        "rector": "vendor/bin/rector",
        "stan": "vendor/bin/phpstan analyse --memory-limit 512M src tests --level 1"
    },
    "authors": [
        {
            "name": "Arthur de Moulins",
            "email": "arthurdemoulins@gmail.com"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.16"
    }
}
